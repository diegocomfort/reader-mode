{
	"manifest_version": 2,
	"name": "Reader Mode",
	"version": "0.0",
	"description": "Squishes site into the middle of the screen",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_title": "Enable Reader Mode"
	},
	"permissions": ["activeTab"]
}
